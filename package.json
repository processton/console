{
  "name": "my-first-gatsby-site",
  "version": "1.0.0",
  "private": true,
  "description": "My First Gatsby Site",
  "author": "Megan Sullivan",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build:css": "postcss src/styles/global.css -o src/common.css",
    "watch:css": "postcss src/styles/global.css -o src/common.css -w",
    "dev": "gatsby develop",
    "develop": "NODE_OPTIONS='--no-deprecation' gatsby develop -H 0.0.0.0",
    "start": "NODE_OPTIONS='--no-deprecation' gatsby develop -H 0.0.0.0",
    "build": "NODE_OPTIONS='--no-deprecation' gatsby build",
    "serve": "NODE_OPTIONS='--no-deprecation' gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "classnames": "^2.5.1",
    "gatsby": "^5.14.1",
    "gatsby-cli": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-mdx": "^5.14.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-source-custom-api": "^2.3.6",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "gatsby-plugin-postcss": "^6.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "linux"
      ],
      "cpu": [
        "x64",
        "x64 sharp",
        "arm64"
      ]
    }
  }
}
